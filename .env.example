# Supabase Configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# OpenAI Configuration (for AI summaries and keyword extraction)
# If not set, the daemon will use a simplified fallback mechanism
OPENAI_API_KEY=sk-your_openai_api_key_here

# Development Mode
# Set to 'true' to use fallback summarization instead of OpenAI API
# This is useful for development to avoid API costs and rate limits
# Default: false
DEVELOPMENT=false

# Claude Code Home Directory (optional)
CLAUDE_CODE_HOME=
